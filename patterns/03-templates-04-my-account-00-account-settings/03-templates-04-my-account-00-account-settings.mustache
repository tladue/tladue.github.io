&lt;section class=&quot;container&quot; role=&quot;main&quot;&gt;
	&lt;h1&gt;Account Settings&lt;/h1&gt;

	&lt;p&gt;The payment and transportation selects made on this screen will be used as default selections for all vehicle purchases made in the system.&lt;/p&gt;
	
	&lt;form id=&quot;account-settings-form&quot; class=&quot;account-settings-form&quot;&gt;
		&lt;h2&gt;Default Payment Method Options&lt;/h2&gt;

	
		&lt;div class=&quot;row&quot;&gt;
			&lt;div class=&quot;form-group select col-sm-6&quot;&gt;
				&lt;div class=&quot;alt-links pull-right above&quot;&gt;
					&lt;a href=&quot;#&quot;&gt;Edit payment method&lt;/a&gt;
				&lt;/div&gt;
				&lt;label class=&quot;control-label&quot; for=&quot;pm-method&quot;&gt;Payment Method&lt;/label&gt;
				&lt;select class=&quot;form-control&quot; id=&quot;pm-method&quot; name=&quot;pm-method&quot;&gt;
					&lt;option&gt;ACH&lt;/option&gt;
				&lt;/select&gt;
			&lt;/div&gt;
		&lt;/div&gt;

		&lt;h2&gt;Default Delivery Options&lt;/h2&gt;

		&lt;div class=&quot;row&quot;&gt;
			&lt;div class=&quot;form-group select col-sm-6 col-md-3&quot;&gt;
				&lt;label class=&quot;control-label&quot; for=&quot;do-provider&quot;&gt;Transportation Provider&lt;/label&gt;
				&lt;select class=&quot;form-control&quot; id=&quot;do-provider&quot; name=&quot;do-provider&quot;&gt;
					&lt;option&gt;Ready Auto&lt;/option&gt;
				&lt;/select&gt;
			&lt;/div&gt;

			&lt;div class=&quot;form-group select col-sm-6 col-md-3&quot;&gt;
				&lt;label class=&quot;control-label&quot; for=&quot;do-type&quot;&gt;Default Delivery Type&lt;/label&gt;
				&lt;select class=&quot;form-control&quot; id=&quot;do-type&quot; name=&quot;do-type&quot;&gt;
					&lt;option&gt;Buyer Location&lt;/option&gt;
				&lt;/select&gt;
			&lt;/div&gt;

			&lt;div class=&quot;form-group select col-sm-12 col-md-6&quot;&gt;
				&lt;div class=&quot;alt-links pull-right above&quot;&gt;
					&lt;a href=&quot;#&quot;&gt;Add a location&lt;/a&gt;
				&lt;/div&gt;
				&lt;label class=&quot;control-label&quot; for=&quot;do-location&quot;&gt;Payment Method&lt;/label&gt;
				&lt;select class=&quot;form-control&quot; id=&quot;do-location&quot; name=&quot;do-location&quot;&gt;
					&lt;option&gt;Hometown Autos, 123 Street St, Atlanta, GA 30312&lt;/option&gt;
				&lt;/select&gt;
			&lt;/div&gt;
		&lt;/div&gt;
		
		&lt;h2&gt;Bidding Profile&lt;/h2&gt;

		&lt;div class=&quot;row&quot;&gt;

			&lt;div class=&quot;col-sm-12&quot;&gt;
				&lt;div class=&quot;form-group&quot;&gt;
					&lt;label class=&quot;control-label&quot;&gt;Match Reserve&lt;/label&gt;
					&lt;div class=&quot;checkbox-group&quot;&gt;
						&lt;input type=&quot;checkbox&quot; id=&quot;mp-match-reserve&quot; name=&quot;mp-match-reserve&quot; checked=&quot;checked&quot; /&gt;
						&lt;label for=&quot;mp-match-reserve&quot;&gt;Match to reserve if maximum bid is greater than reserve price &lt;/label&gt;
					&lt;/div&gt;
				&lt;/div&gt;
			&lt;/div&gt;
		&lt;/div&gt;

		&lt;div class=&quot;bottom-actions&quot;&gt;
			&lt;div class=&quot;row&quot;&gt;
				&lt;div class=&quot;col-xs-6 col-sm-5 col-sm-offset-4 col-md-4 col-md-offset-6&quot;&gt;
					&lt;a href=&quot;#&quot; class=&quot;btn btn-cancel&quot;&gt;Cancel&lt;/a&gt;
				&lt;/div&gt;

				&lt;div class=&quot;col-xs-6 col-sm-3 col-md-2&quot;&gt;
					&lt;button class=&quot;btn&quot;&gt;Submit&lt;/button&gt;
				&lt;/div&gt;
			&lt;/div&gt;
		&lt;/div&gt;

	&lt;/form&gt;

&lt;/section&gt;